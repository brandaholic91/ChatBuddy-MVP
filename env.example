# Chatbuddy MVP Environment Variables
# Másold át ezt a fájlt .env néven és töltsd ki a valós értékekkel

# Environment
ENVIRONMENT=development

# AI API Keys
OPENAI_API_KEY=sk-your-openai-api-key-here-minimum-20-characters
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here-minimum-20-characters

# Database (Supabase)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key-here-minimum-100-characters
SUPABASE_SERVICE_KEY=your-supabase-service-key-here-minimum-100-characters

# Security
SECRET_KEY=your-secret-key-here-minimum-32-characters-long
JWT_SECRET=your-jwt-secret-here-minimum-32-characters-long

# Redis (optional)
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your-redis-password-here-minimum-8-characters

# Rate Limiting
RATE_LIMIT_DEFAULT=100/minute

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080
ALLOWED_HOSTS=localhost,127.0.0.1

# Monitoring (optional)
LOGFIRE_TOKEN=your-logfire-token-here-minimum-20-characters

# Email (optional)
SENDGRID_API_KEY=your-sendgrid-api-key-here-minimum-20-characters

# SMS (optional)
TWILIO_ACCOUNT_SID=your-twilio-account-sid-here-minimum-20-characters
TWILIO_AUTH_TOKEN=your-twilio-auth-token-here-minimum-20-characters

# Development Settings
DEBUG=true
LOG_LEVEL=INFO 